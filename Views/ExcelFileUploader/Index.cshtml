<form method="post" enctype="multipart/form-data" asp-controller="ExcelFileUploader" asp-action="Index">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload your files to BLOB storage(in excel format) :</p>
            <input type="file" name="files" accept="application/excel" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>
